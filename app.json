{
    "name": "barebones nodejs",
    "description": "A barebones Node.js app using Express 4.",
    "keywords": [
      "nodejs",
      "express"
    ],
    "repository": "https://github.com/michaelshobbs/node-js-sample",
    "scripts": {
      "dokku": {
        "predeploy": "touch /app/predeploy.test",
        "postdeploy": [ "printenv", "heroku buildpacks"]
      }
    },
    "env": {
      "SECRET_TOKEN": {
        "description": "A secret key for verifying the integrity of signed cookies.",
        "value": "secret"
      },
      "WEB_CONCURRENCY": {
        "description": "The number of processes to run.",
        "generator": "echo 5"
      }
    },
    "image": "gliderlabs/herokuish",
    "addons": [
      "dokku-postgres",
      "dokku-redis"
    ],
    "buildpacks": [
      {
        "url": "https://github.com/jontewks/puppeteer-heroku-buildpack.git"
      }
    ]
  }
  